{
    "name": "screepsts",
    "version": "!.0.0",
    "scripts": {
        "lint": "eslint \"src/***/*.ts\"",
        "build": "rollup -c",
        "push-main": "rollup -c --environment DEST:main",
        "push-sim": "rollup -c --environment DEST:sim",
        "push-season": "rollup -c --environment DEST:season",
        "push-pserver": "rollup -c --environment DEST:pserver",
        "push-local": "rollup -c --environment DEST:local",
        "watch-main": "rollup -cw --environment DEST:main",
        "watch-sim": "rollup -cw --environment DEST:sim",
        "watch-season": "rollup -cw --environment DEST:season",
        "watch-pserver": "rollup -cw --environment DEST:pserver",
        "watch-local": "rollup -cw --environment DEST:local"
    },
    "devDependencies": {
        "@types/node": "^16.11.4",
        "@types/screeps": "^3.2.4",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@types/chai": "^4.2.22",
        "@types/lodash": "^4.14.176",
        "@types/mocha": "^9.0.0",
        "@types/sinon": "^10.0.5",
        "@types/sinon-chai": "^3.2.5",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@typescript-eslint/typescript-estree": "^5.1.0",
        "chai": "^4.3.4",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-prettier": "^4.0.0",
        "lodash": "^4.17.21",
        "mocha": "^9.1.3",
        "prettier": "^2.4.1",
        "rollup": "^2.58.0",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-screeps": "^1.0.1",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "sinon": "^11.1.2",
        "sinon-chai": "^3.7.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.11.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "source-map": "^0.6.1"
    }
